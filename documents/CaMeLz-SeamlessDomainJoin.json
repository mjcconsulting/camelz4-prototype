{
  "schemaVersion": "1.0",
  "description": "Join Instances to Domain @directory_domain@ (@directory_id@)",
  "runtimeConfig": {
    "aws:domainJoin": {
      "properties": {
        "directoryId": "@directory_id@",
        "directoryName": "@directory_domain@",
        "dnsIpAddresses": [
          "@directory_dc_ip1@",
          "@directory_dc_ip2@"
        ]
      }
    }
  }
}
